{
    "name": "oauth",
    "version": "1.0.0",
    "devDependencies": {
        "@cloudflare/kv-asset-handler": "^0.3.0",
        "axios": "^1.3.6",
        "chai": "^4.3.7",
        "cross-env": "^7.0.3",
        "mocha": "^10.2.0",
        "prompt-sync": "^4.2.0",
        "wrangler": "2.9.0"
    },
    "private": true,
    "scripts": {
        "start": "wrangler dev",
        "publish": "wrangler publish --env production",
        "start:local": " wrangler dev --local --persist --ip 127.0.0.1",
        "test:api": "cross-env AUTH_URL=http://127.0.0.1:8787 mocha --recursive './api-tests/**/*.spec.js'",
        "create:local-user": "node ./helperScripts/generateUser.js",
        "create:local-client": "node ./helperScripts/addLocalClient.js",
        "create:key-pair": "node ./helperScripts/generateKey.js",
        "create:api-test-setup": "node ./helperScripts/setupApiTests.js"
    }
}
