{
  "name": "oauth",
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@cloudflare/kv-asset-handler": "0.3.1",
    "axios": "1.3.6",
    "chai": "4.3.7",
    "cross-env": "7.0.3",
    "mocha": "10.2.0",
    "prompt-sync": "4.2.0",
    "sinon": "17.0.1",
    "wrangler": "3.26.0"
  },
  "private": true,
  "scripts": {
    "start": "wrangler dev",
    "publish": "wrangler publish --env production",
    "pretlint": "npx @biomejs/biome check --apply ./src",
    "start:local": " wrangler dev --local --persist --ip 127.0.0.1",
    "test:unit": "mocha --recursive --require './test/unit/mochaHooks.js' './test/unit/**/*.spec.js'",
    "create:local-user": "node ./helperScripts/generateUser.js",
    "create:local-client": "node ./helperScripts/addLocalClient.js",
    "create:key-pair": "node ./helperScripts/generateKey.js",
    "create:api-test-setup": "node ./helperScripts/setupApiTests.js"
  }
}
